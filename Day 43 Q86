#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    int i = 0, j;

    scanf("%s", str);
    j = strlen(str) - 1;

    while(i < j) {
        if(str[i] != str[j]) {
            printf("Not Palindrome");
            return 0;
        }
        i++;
        j--;
    }

    printf("Palindrome");
    return 0;
}
