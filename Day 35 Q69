#include <stdio.h>

int main()
{
    int n,secmax;
    printf("Enter the number of element in array..");
    scanf("%d",&n);
    int array[n];
    printf("ENTER THE ARRAY IN ELEMENT..");
    for (int i=0;i<n;i++)
    {
        scanf("%d",&array[i]);
    }
    int max=array[0];
    for(int j=0;j<n;j++)
    {
        if (max<array[j])
        {
            secmax=max;
            max=array[j];
        }
        else if (secmax<array[j] && array[j]!=max)
        {
            secmax=array[j];
        }
        else
        {
            continue;
        }
        
    }
    printf("THE SECOND MAXIMUM NUMBER IN THE ARRAY IS %d",secmax);
    
}
