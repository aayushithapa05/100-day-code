#include <stdio.h>

int main()
{
    int n,sum=0;
    printf("Enter the value of n..");
    scanf("%d",&n);
    for (int i=1;i<=n;i++)
    {
        if(i%2==0)
        {
            sum=sum;
        }
        else
        {
            sum=sum+i;
        }
    }
    printf("The sum of odd numbers is %d",sum);
    return 0;
}
