#include <stdio.h>

int main() {
    int num1,num2,sum,diff,prdct,div;
    printf("Enter 2 numbers");
    scanf("%d%d",&num1,&num2);
    sum=num1+num2;
    printf("The sum of the numbers is %d\n",sum);
    diff=num1-num2;
    printf("The diffrence of two numbers is %d\n",diff);
    prdct=num1*num2;
    printf("The product of two numbers is %d\n",prdct);
    div=num1/num2;
    printf("The division of two numbers is %d\n",div);
    
    return 0;
}
