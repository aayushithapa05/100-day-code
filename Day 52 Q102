#include <stdio.h>

int main() {
    int n, x, i;
    int arr[200];
    scanf("%d", &n);
    for(i = 0; i < n; i++)
        scanf("%d", &arr[i]);
    scanf("%d", &x);

    int l = 0, r = n - 1, ans = -1;
    while(l <= r) {
        int m = (l + r) / 2;
        if(arr[m] >= x) {
            ans = m;
            r = m - 1;
        } else
            l = m + 1;
    }

    printf("%d", ans);
    return 0;
}
