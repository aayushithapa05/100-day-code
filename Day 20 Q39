#include <stdio.h>

int main()
{
    int num,d,sum=0;
    printf("Enter the number..");
    scanf("%d",&num);
    while (num>0)
    {
        d=num%10;
        if (d%2==0)
        {
            sum=sum;
        }
        else
        {
            sum=sum+d;
        }
        num=num/10;
    }
    printf("The sum of the odd digits of the number is %d",sum);
    return 0;
}
