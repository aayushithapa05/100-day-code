#include <stdio.h>

int main() {
    int r, c;
    scanf("%d %d", &r, &c);

    int a[r][c];
    for(int i = 0; i < r; i++)
        for(int j = 0; j < c; j++)
            scanf("%d", &a[i][j]);

    for(int start = 0; start < r; start++) {
        int i = start, j = 0;
        while(i >= 0 && j < c) {
            printf("%d ", a[i][j]);
            i--; j++;
        }
    }

    for(int start = 1; start < c; start++) {
        int i = r-1, j = start;
        while(i >= 0 && j < c) {
            printf("%d ", a[i][j]);
            i--; j++;
        }
    }

    return 0;
}
