#include <stdio.h>

int main() 
{
    int temp,a,b;
    printf("\nEnter a value 1 i.e A...");
    scanf("%d",&a);
    printf("\nEnter a value 2 i.e B...");
    scanf("%d",&b);
    a=a+b;
    b=a-b;
    a=a-b;
    printf("\nEnter a value 1 i.e A...%d\n",a);
    printf("\nEnter a value 2 i.e B...%d",b);
    
    return 0;
}
