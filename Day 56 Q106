#include <stdio.h>

int main() {
    int n, i, j;
    int a[300];
    scanf("%d", &n);

    for(i = 0; i < n; i++)
        scanf("%d", &a[i]);

    for(i = 0; i < n; i++) {
        int next = -1;
        for(j = i + 1; j < n; j++) {
            if(a[j] > a[i]) {
                next = a[j];
                break;
            }
        }
        if(i == n - 1)
            printf("%d", next);
        else
            printf("%d,", next);
    }

    return 0;
}
