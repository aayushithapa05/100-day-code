#include <stdio.h>

int main() {
    char str[200], res[200];
    int i, j = 0;

    fgets(str, 200, stdin);

    for(i = 0; str[i] != '\0'; i++) {
        char c = str[i];
        if(c!='a' && c!='e' && c!='i' && c!='o' && c!='u' &&
           c!='A' && c!='E' && c!='I' && c!='O' && c!='U') {
            res[j++] = c;
        }
    }

    res[j] = '\0';
    printf("%s", res);
    return 0;
}
