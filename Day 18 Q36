#include <stdio.h>

int main()
{
    int num1,num2,hcf;
    printf("Enter the number 1 ..");
    scanf("%d",&num1);
    printf("Enter the number 2 ..");
    scanf("%d",&num2);
    for (int i=1;i<=num1/2;i++)
    {
        if(num1%i==0 && num2%i==0)
        {
            hcf=i;
        }
    }
    printf("The Hcf of two number is %d",hcf);
    
    return 0;
}
