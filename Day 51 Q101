#include <stdio.h>

int first(int a[], int n, int t) {
    int l = 0, r = n - 1, ans = -1;
    while (l <= r) {
        int m = (l + r) / 2;
        if (a[m] == t) {
            ans = m;
            r = m - 1;
        } else if (a[m] > t)
            r = m - 1;
        else
            l = m + 1;
    }
    return ans;
}

int last(int a[], int n, int t) {
    int l = 0, r = n - 1, ans = -1;
    while (l <= r) {
        int m = (l + r) / 2;
        if (a[m] == t) {
            ans = m;
            l = m + 1;
        } else if (a[m] > t)
            r = m - 1;
        else
            l = m + 1;
    }
    return ans;
}

int main() {
    int n, t, i;
    int a[200];
    scanf("%d", &n);
    for(i = 0; i < n; i++)
        scanf("%d", &a[i]);
    scanf("%d", &t);

    int f = first(a, n, t);
    int l = last(a, n, t);

    if (f == -1)
        printf("-1 -1");
    else
        print
