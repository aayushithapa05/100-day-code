#include <stdio.h>

int prime (int num)
{
    int ans;
    for (int i=2;i<num;i++)
    {
        if(num%i==0)
        {
            ans=0;
            break;
        }
        else
        {
            ans=1;
        }
    }
    return ans;
}
int main()
{
    int n,ans;
    printf("Enter the value of n..");
    scanf("%d",&n);
    printf("The prime numbers are.. 2");
    for (int i=2;i<=n;i++)
    {
        ans=prime(i);
        if(ans==1)
        {
            printf("%d\t",i);
        }
    }
    
}
