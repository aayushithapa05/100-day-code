#include <stdio.h>

int main() {
    char str[200];
    int freq[26] = {0};
    int i;

    fgets(str, 200, stdin);

    for(i = 0; str[i] != '\0'; i++) {
        if(str[i] >= 'a' && str[i] <= 'z') {
            int idx = str[i] - 'a';
            freq[idx]++;
            if(freq[idx] == 2) {
                printf("%c", str[i]);
                return 0;
            }
        }
    }

    printf("No repeating lowercase alphabet");
    return 0;
}
