#include <stdio.h>

int main() 
{
    int n,sum=0;
    printf("Enter the numbers of elements..");
    scanf("%d",&n);
    int array[n];
    printf("Enter the Elements..");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&array[i]);
    }
    int max=array[0],min=array[0];
    for(int i=0;i<n;i++)
    {
        if(max<array[i])
        {
            max=array[i];
        }
        if(min>array[i])
        {
            min=array[i];
        }
    }
    printf("The maximum number from the given elements in tha array is %d\n",max);
    printf("The minimum number from the given elements in tha array is %d",min);
    return 0;
}
