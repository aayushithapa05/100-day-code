#include <stdio.h>

int main() 
{
    int r,c;
    printf("Enter the number of rows..");
    scanf("%d",&r);
    printf("Enter the number of column..");
    scanf("%d",&c);
    int mat[r][c];
    for (int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        {
            printf("Enter the emenemts of row %d .. ",i+1);
            scanf("%d",&mat[i][j]);
        }
    }
    printf("Matrix: \n")
    for (int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        {
            printf("%d\t",mat[i][j]);
        }
        printf("\n");
    }
    
    
    return 0;
}
