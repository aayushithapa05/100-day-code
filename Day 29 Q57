#include <stdio.h>

int main() 
{
    int n,sum=0;
    printf("Enter the numbers of elements..");
    scanf("%d",&n);
    int array[n];
    printf("Enter the Elements..");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&array[i]);
    }
    for(int i=0;i<n;i++)
    {
        sum=sum+array[i];
    }
    printf("The sum of elements of the given array is %d",sum);
    
    return 0;
}
