#include <stdio.h>

int main() 
{
    char ch;
    printf("Enter the charecter..");
    scanf("%c",&ch);
    if ('a'<=ch && ch<='z')
    {
        printf("The charecter is Lower Case");
    }
    else if('A'<=ch && ch<='Z')
    {
        printf("The charecter is Upper Case");
    }
    else if('0'<=ch && ch<='9')
    {
        printf("The charecter is Digit");
    }
    else
    {
        printf("The charecter is Special Charecter");
    }
    
    return 0;
}
